def count(n):
    for i in range(2,n):
        if n%i==0:
            return False
    return True
n=int(input('Enter n: '))
c=0
for i in range(2,n+1):
    if count(i):
        c=c+1
print('Number of prime numbers: ',c)
Enter n: 26
Number of prime numbers:  9

