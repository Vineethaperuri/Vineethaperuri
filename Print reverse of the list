# print reverse the list
l=[]
l1=[]
n=int(input("Enter the n: "))
for i in range(0,n):
    x=int(input())
    l.append(x)
for i in range(0,n):
    j=i+1
    l1.insert(-i,l[i])
print(l)
print(l1)
